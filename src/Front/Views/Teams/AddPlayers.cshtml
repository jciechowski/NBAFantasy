@using NBAFantasy.Models
@model IEnumerable<Player>

@{
    ViewBag.Title = "Teams";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Team details</h2>

<p>
    <a asp-action="AddPlayers">Add players</a>
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayFor(model => model.GM)
        </th>
    </tr>
</table>
